{
  "manifest_version": 3,
  "name": "Laravel Page Sender",
  "version": "1.0",
  "description": "Scrapes profile data from specific websites.",
  "permissions": [
    "scripting",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "http://127.0.0.1:8000/*",
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon-default.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}